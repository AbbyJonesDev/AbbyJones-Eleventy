<ol reversed class="talklist" style="counter-reset: start-from {{ talkslist.length + 1 }}">
{% for talk in talkslist | reverse %}
  <li class="talklist-item{% if talk.url == url %} talklist-item-active{% endif %}">
    <a href="{{ talk.url | url }}" class="talklist-link">{% if talk.data.title %}{{ talk.data.title }}{% else %}<code>{{ talk.url }}</code>{% endif %}</a>
    <time class="talklist-date" datetime="{{ talk.date | htmlDateString }}">{{ talk.date | readableDate }}</time>
    {% for tag in talk.data.tags %}
      {%- if tag != "talks" -%}
      {% set tagUrl %}/tags/{{ tag }}/{% endset %}
      <a href="{{ tagUrl | url }}" class="tag">{{ tag }}</a>
      {%- endif -%}
    {% endfor %}
  </li>
{% endfor %}
</ol>
