---
permalink: /tag-list/
layout: layouts/home.njk
---
<h1>Tags</h1>

<div class="tag-list">
  {% for tag in collections.tagList %}
    {%- if tag != "" -%}
      {% set tagUrl %}/tags/{{ tag | slugify }}/{% endset %}
      <a href="{{ tagUrl | url }}" class="tag">{{ tag }}</a>
    {%- endif -%}
  {% endfor %}
</div>
